         <image>
            <Image url="@../../../../../Images/email-icon--100-flat-vol-2-iconset--graphicloads-18.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="70.0" fitWidth="81.0" layoutX="20.0" layoutY="202.0">
