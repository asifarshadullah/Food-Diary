            <TextField fx:id="name" prefHeight="35.0" prefWidth="300.0" promptText="Your Name" />
            <TextField fx:id="email" prefHeight="35.0" prefWidth="300.0" promptText="Your E-mail" />
            <PasswordField fx:id="pass" prefHeight="35.0" prefWidth="300.0" promptText="Your Password" />
