<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SearchByNameController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="600.0">
         <image>
            <Image url="@../../../../../Images/b.png" />
         </image>
      </ImageView>
      <TextField fx:id="txt" layoutX="33.0" layoutY="142.0" onAction="#Search" prefWidth="300.0" promptText="Enter Restaurant Name">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button layoutX="490.0" layoutY="34.0" mnemonicParsing="false" onAction="#LogOut" text="Log Out">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button layoutX="133.0" layoutY="410.0" mnemonicParsing="false" onAction="#Search" prefHeight="45.0" prefWidth="142.0" text="Search">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button layoutX="73.0" layoutY="35.0" mnemonicParsing="false" onAction="#goToPrevious" prefHeight="30.0" prefWidth="163.0" text="Go to previous page">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <ListView fx:id="list" layoutX="33.0" layoutY="172.0" prefHeight="200.0" prefWidth="300.0" />
      <TableView fx:id="billTable" layoutX="382.0" layoutY="142.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="itemBill" prefWidth="121.0" text="Items" />
          <TableColumn fx:id="priceBill" prefWidth="77.0" text="Price" />
        </columns>
      </TableView>
      <Button layoutX="454.0" layoutY="372.0" mnemonicParsing="false" onAction="#removeFromList" text="Remove from list">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button layoutX="382.0" layoutY="372.0" mnemonicParsing="false" onAction="#submitBill" text="Submit">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <ImageView fitHeight="51.0" fitWidth="55.0" layoutX="435.0" layoutY="24.0">
         <image>
            <Image url="@../../../../../Images/Apps-Dialog-Logout-icon.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="85.0" fitWidth="92.0" layoutX="50.0" layoutY="398.0">
         <image>
            <Image url="@../../../../../Images/71646d7673e8847ab07b3b7e78928777-search-circle-icon-by-vexels.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="51.0" fitWidth="55.0" layoutX="14.0" layoutY="25.0">
         <image>
            <Image url="@../../../../../Images/Backward-512.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
